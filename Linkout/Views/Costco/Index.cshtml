@*Costco Index*@

@section styles{
    @Styles.Render("~/styles/cc")
}

<div id="costco-container" class="costco-container" ng-app="costco" ng-controller="CostcoCtrl" zb-route-styles>
    <div id="cc-head">
        Roadwire
        <div id="cc-logo">
            Costco
        </div>
    </div>

    <div ng-view id="costco-view" class="costco-view"></div>
    <footer class="costco-footer">
        &copy; @DateTime.Now.Year - Roadwire LLC
    </footer>
</div>


@section scripts{
    
    @Scripts.Render("~/bundles/cc/jquery")
    @Scripts.Render("~/bundles/cc/angular")
    @Scripts.Render("~/bundles/cc/bootstrap")
    @Scripts.Render("~/bundles/cc/ui-boots")
    @Scripts.Render("~/bundles/cc/angstyles")
    @Scripts.Render("~/bundles/cc/animate")
    @Scripts.Render("~/bundles/cc/rwutils")
    @Scripts.Render("~/bundles/cc/costco")
}
