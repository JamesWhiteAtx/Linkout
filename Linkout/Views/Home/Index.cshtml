@{
    ViewBag.Title = "Roadwire";
}


@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@Html.ActionLink("Costco Linkout Site", "Index", "Linkout")</h1>
                <h2>Go to the Costco Linkout site.</h2>
            </hgroup>
        </div>
    </section>
}

<h3>Addional Development Site Optons:</h3>

<ol class="round">
@if (Request.IsAuthenticated)
{
    <li class="one">
        <h5> @Html.ActionLink("Confgure", "Index", "Configure") </h5>
    </li>
    <li class="two">
        <h5> @Html.ActionLink("Log out ", "LogOff", "Account")  (User: @User.Identity.Name)</h5>
    </li>        
}
else
{
    <li class="one">
    <h5> @Html.ActionLink("Log In", "LogOn", "Account") </h5>
    </li>
}
  
</ol>