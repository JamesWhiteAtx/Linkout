@* Views\Mock *@
<!DOCTYPE html>
<html>
<head>
    <link rel="SHORTCUT ICON" href="@Url.Content("~/Images/Mock/favicon.ico")"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/css/bootstrap-cust")
    @Styles.Render("~/css/fonts/font-awesome")
    @Styles.Render("~/css/jquery/jqueryuicust")

    <style>
        body {
            padding-top: 60px; /* 60px to make the container go all the way to the bottom of the topbar */
            background-color: rgb(239, 239, 239);
        }
        .mock-cont 
        {
            background-color: #fff;
            margin-top: 20px;
        }
    </style>

    @RenderSection("styles", required: false)   
</head>

<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          @*<a class="brand" href="#">Project name</a>*@
          <div class="nav-collapse collapse">
            <ul class="nav">
				<li><a href="http://www.costco.com/TopCategories?langId=-1&amp;storeId=10301&amp;catalogId=10701&amp;cm_re=Common-_-Top_Nav-_-Home">Shop</a></li>
	        	<li><a href="http://www2.costco.com/Browse/BDLanding.aspx?lang=en-US&amp;cm_re=Common-_-Top_Nav-_-Business_Delivery">Business Delivery</a></li>
		       	<li><a href="http://www2.costco.com/Service/FeaturePage.aspx?ProductNo=11486072&amp;cm_re=Common-_-Top_Nav-_-Pharmacy">Pharmacy</a></li>
	       		<li><a href="http://www.costco.com/services.html?cm_re=Common-_-Top_Nav-_-Services">Services</a></li>
	       		<li><a href="http://www.costcophotocenter.com/account/default.aspx">Photo</a></li>
	      		<li><a href="http://www.costcotravel.com?cm_re=Common-_-Top_Nav-_-Travel">Travel</a></li>
		    </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>
        
    <section >
        <div class="container">

            <div id="logo-top" class="container">
                <div class="pull-left">
                    <a href="/Mock">
                        <img src="@Url.Content("~/Images/Mock/costco_wholesale_214_64.png")" alt="Costco.com"/>
                    </a>
                </div>
            </div>
        </div> 
        <div class="mock-cont container">
            @RenderBody()
        </div>
    </section>
    
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryuicust")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/angular-resource")
    @Scripts.Render("~/bundles/bootstrapjs")
    @Scripts.Render("~/bundles/configure")

    @RenderSection("scripts", required: false)
</body>

</html>

